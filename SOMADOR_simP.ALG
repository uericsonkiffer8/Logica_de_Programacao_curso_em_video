algoritmo "Somador"

var
N, cont, S : inteiro


inicio
      S <- 0
      cont <- 1
      enquanto ( cont <= 5) faca
      escreval("Digite o ", cont, " o. valor ==> ")
      leia(N)
      s <- s + N
      cont <- cont + 1
      FimEnquanto
      escreval("  A soma de todos os valores  digitados é", S)

fimalgoritmo